{
  "name": "plan2net/single-site",
  "license": "GPL-3.0",
  "description": "Optimize TYPO3 Site handling for a single Site installation",
  "type": "typo3-cms-extension",
  "keywords": [
    "TYPO3",
    "site",
    "performance"
  ],
  "homepage": "https://www.plan2.net/",
  "authors": [
    {
      "name": "Wolfgang Klinger",
      "role": "Lead Developer"
    }
  ],
  "require": {
    "php": ">=8.2",
    "typo3/cms-core": "^13.4"
  },
  "autoload": {
    "psr-4": {
      "Plan2net\\SingleSite\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "single_site"
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  }
}
